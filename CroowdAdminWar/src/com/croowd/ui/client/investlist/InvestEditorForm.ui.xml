<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel styleName="fullbox" height="100%"
		width="100%">
		<g:HTMLPanel styleName="header1">
			<div>DATA PROSPEK</div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="headerspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel styleName="widgetbox" ui:field="prospectWidget" />
		<g:HTMLPanel styleName="widgetboxspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="header1">
			<div>DATA INVESTASI</div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="headerspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel styleName="widgetbox">
			<g:HTMLPanel styleName="fullbox">
				<table class="fullbox">
					<tr>
						<td class="halfbox">
							<div class="compositebox">
								<div class="fieldtitle">Invester</div>
								<g:Label styleName="fieldlabel" ui:field="memberName" />
							</div>
							<div class="compositebox">
								<div class="fieldtitle">Nilai investasi</div>
								<g:NumberLabel styleName="fieldlabel" ui:field="value"
									customFormat="#,##0.00" />
							</div>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:HorizontalPanel>
		<g:HTMLPanel styleName="widgetboxspacer">
			<div></div>
		</g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Button styleName="fieldbigbutton" ui:field="btnSave"
				text="Setujui" />
			<g:HTMLPanel styleName="widgetboxspacer">
				<div class="fieldspacer"></div>
			</g:HTMLPanel>
			<g:cell width="100%" horizontalAlignment="ALIGN_RIGHT">
				<g:Button styleName="fieldbigbutton" ui:field="btnBack"
					text="Kembali" />
			</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 