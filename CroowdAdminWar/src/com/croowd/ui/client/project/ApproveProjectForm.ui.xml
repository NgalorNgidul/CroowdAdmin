<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:edv="urn:import:org.simbiosis.ui.gwt.client.editor">
	<ui:style>
	
	</ui:style>

	<!-- <g:TabLayoutPanel barUnit='PX' barHeight='40'
		ui:field="viewCust"> 
		<g:tab>-->
			<g:ScrollPanel>
				<g:VerticalPanel styleName="fullbox">
					<g:HTMLPanel styleName="header1">
						<di>Approve Project</di>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="headerspacer">
						<div></div>
					</g:HTMLPanel>
					<g:HorizontalPanel styleName="widgetbox">
						<g:HTMLPanel styleName="fullbox">
							<table class="tblDomain">
								<tr>
									<td class="tdHeader" colspan="2">Customer Information</td>
								</tr>
								<tr>
									<td class="tdTitle">Name</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="nasabah" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">CIF</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="cif" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">Place of Bird</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="pob" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">Date of Bird</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="dob" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">Address</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="address" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">Address1</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="address1" />
									</td>
								</tr>
								<tr>
									<td class="tdTitle">Phone</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="phone" />
									</td>
								</tr>
								<tr>
									<td class="tdHeader" colspan="2">Project Information</td>
								</tr>
								<tr>
									<td class="tdTitle">Project Name</td>
									<td class="tdContent">
										<g:Label styleName="fieldlabel" ui:field="projectName" />
									</td>
								</tr>
								
							</table>
							<table>
								<tr>
									<td>
										<g:Button ui:field="approve" text="Approve"></g:Button>
									</td>
									<td>
										<g:Button ui:field="reject" text="Reject"></g:Button>
									</td>
								</tr>
							</table>
						</g:HTMLPanel>

					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:ScrollPanel>
		<!-- </g:tab>


	</g:TabLayoutPanel> -->

</ui:UiBinder> 